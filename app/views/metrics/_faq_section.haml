.row
    .col-md-3
        .card.shadow-sm.sticky-top.mb-3
            .card-header
                %h5 FAQ Clicks
            .card-body
                %table.table
                    %tr
                        %th Question
                        %th Clicks
                    %tbody
                        -@questions.keys.each do |k|
                            %tr
                                %td
                                    =k
                                %td
                                    =questions[k]
    .col-md-9.mb-3
        .card.shadow-sm
            .card-header
                %h5 FAQ Clicks
            .card-body
                = line_chart questions_time.group_by_day(:created_at).count
